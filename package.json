  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@beecode/msh-node-log": "2.0.2",
    "@beecode/msh-test-contractor": "1.3.1-alpha",
    "rxjs": "^7.3.0"
  },
  "description": "This project is intended to be used in typescript project to help with app initialization.",
  "devDependencies": {
    "@commitlint/cli": "13.1.0",
    "@commitlint/config-conventional": "13.1.0",
    "@commitlint/prompt": "13.1.0",
    "@istanbuljs/nyc-config-typescript": "1.0.1",
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/commit-analyzer": "9.0.1",
    "@semantic-release/exec": "5.0.0",
    "@semantic-release/git": "9.0.1",
    "@semantic-release/github": "8.0.0",
    "@semantic-release/release-notes-generator": "10.0.2",
    "@types/dotenv": "8.2.0",
    "@types/jest": "27.0.1",
    "@types/node": "16.9.2",
    "@types/proxyquire": "1.3.28",
    "@typescript-eslint/eslint-plugin": "4.31.1",
    "@typescript-eslint/parser": "4.31.1",
    "codecov": "3.8.3",
    "commitizen": "4.2.4",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.24.2",
    "husky": "7.0.2",
    "jest": "27.2.0",
    "jest-extended": "0.11.5",
    "markdown-toc": "1.2.0",
    "nyc": "15.1.0",
    "pinst": "2.1.6",
    "prettier": "2.4.1",
    "proxyquire": "2.1.3",
    "replace-json-property": "1.7.1",
    "rimraf": "3.0.2",
    "semantic-release": "18.0.0",
    "source-map-support": "0.5.20",
    "ts-cleaner": "1.0.5",
    "ts-jest": "27.0.5",
    "ts-node": "10.2.1",
    "tsc-watch-2": "0.0.2",
    "typescript": "4.4.3"
  },
  "files": [
    "lib",
    "package-lock.json"
  ],
  "homepage": "https://github.com/beecode-rs/msh-node-app#readme",
  "keywords": [],
  "license": "mit",
  "main": "./lib/index.js",
  "name": "@beecode/msh-node-app",
  "repository": {
    "type": "git",
    "url": "https://github.com/beecode-rs/msh-node-app.git"
  },
  "resolutions": {
    "lodash": "^4.17.21",
    "ssri": "^8.0.1",
    "y18n": "^5.0.8"
  },
  "scripts": {
    "audit:fix": "npx npm-force-resolutions",
    "build": "npm run clean && tsc -p ./tsconfig-build.json",
    "build:watch": "npm run tsc:watch -- --preserveWatchOutput --onSuccess \"npm run tsc:cleaner\"",
    "bump-version": "rjp package.json version",
    "clean": "rimraf ./lib/*",
    "commit": "git-cz",
    "coverage": "nyc npm run test:unit",
    "coverage:publish": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "doc:toc": "markdown-toc -i ./README.md",
    "eslint": "eslint . --ext .ts --quiet",
    "eslint:fix": "npm run eslint -- --fix",
    "lint": "npm run eslint && npm run prettier",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "postinstall": "husky install",
    "postpublish": "pinst --enable",
    "prepublishOnly": "pinst --disable",
    "prettier": "prettier --check \"./src/**/*.{ts,json}\"",
    "prettier:fix": "prettier --write \"./src/**/*.{ts,json}\"",
    "semantic-release": "semantic-release",
    "semantic-release:check": "npm run semantic-release -- --dry-run --no-ci",
    "test": "npm run test:unit -- --forbid-only && npm run test:int -- --forbid-only",
    "test:int": "#",
    "test:unit": "jest",
    "tsc": "tsc -p ./tsconfig-build.json",
    "tsc:check": "npm run tsc -- --noEmit",
    "tsc:cleaner": "ts-cleaner --dist build",
    "tsc:cleaner:watch": "npm run tsc:cleaner -- --watch",
    "tsc:watch": "tsc-watch -p tsconfig-build.json"
  },
  "typings": "./lib/index.d.ts",
  "version": "0.1.1"
}
